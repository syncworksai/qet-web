{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/static-build" }],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",

  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://quantum-edge-fx.onrender.com/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
